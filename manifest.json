{
  "name": "Cashback checker",
  "version": "0.1",
  "manifest_version": 2,

  "browser_action": {
    "default_icon": "crown48.png",
    "default_popup": "popup.html"
  },
  
  "background": {
    "scripts": ["background.js"]
  },
  
  "icons": {
    "16": "crown16.png",
    "48": "crown48.png",
    "128": "crown128.png"
  },

  "permissions": [
		"tabs",
		"activeTab",
		"http://*/*",
        "https://*/*",
		"https://cashback.ru/*",
		"storage" 
  ]
  
}